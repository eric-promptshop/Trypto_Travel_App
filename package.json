{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:dev": "next lint --config .eslintrc.development.json",
    "lint:prod": "next lint --config .eslintrc.production.json",
    "lint:fix": "next lint --fix",
    "lint:fix:dev": "next lint --fix --config .eslintrc.development.json",
    "lint:fix:prod": "next lint --fix --config .eslintrc.production.json",
    "lint:cache:clear": "next lint --cache --cache-location .eslintcache",
    "lint:staged": "lint-staged",
    "type-check": "tsc --noEmit",
    "type-check:jest": "tsc --noEmit -p tsconfig.jest.json",
    "type-check:all": "npm run type-check && npm run type-check:jest",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "lhci": "lhci autorun",
    "setup:supabase": "node scripts/setup-supabase-env.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:seed-admin": "ts-node scripts/seed-admin-data.ts",
    "db:seed-demo": "node scripts/seed-demo-users.js",
    "db:seed-tour-operator": "ts-node scripts/seed-tour-operator-demo-data.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "env:staging": "echo 'Switching to staging...' && sed -i '' 's/NEXT_PUBLIC_ENVIRONMENT=.*/NEXT_PUBLIC_ENVIRONMENT=staging/' .env.local",
    "env:production": "echo 'Switching to production...' && sed -i '' 's/NEXT_PUBLIC_ENVIRONMENT=.*/NEXT_PUBLIC_ENVIRONMENT=production/' .env.local",
    "vercel:setup": "./scripts/vercel-env-setup.sh",
    "vercel:pull": "vercel env pull .env.local",
    "deploy:preview": "vercel",
    "deploy:production": "vercel --prod",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:install": "playwright install",
    "test:accessibility:playwright": "playwright test tests/accessibility/",
    "test:mobile": "playwright test tests/mobile/ --project=mobile",
    "test:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:security": "node tests/security/security-audit.js",
    "test:security:scan": "npm audit --audit-level moderate && npm run test:security",
    "pre-commit": "lint-staged",
    "pre-push": "npm run type-check && npm run test",
    "validate": "npm run type-check && npm run lint && npm run test && npm run build",
    "test:performance": "node tests/performance/benchmark.js",
    "test:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=tests/performance/lighthouse-report.json",
    "test:all": "npm run test && npm run test:e2e && npm run test:security && npm run test:performance",
    "test:ci": "npm run test:coverage && npm run test:e2e && npm run test:accessibility:playwright && npm run test:security",
    "test:production-ready": "npm run test:all && npm run lhci",
    "staging:deploy": "cross-env NODE_ENV=staging npm run build && npm run deploy:preview",
    "staging:test": "wait-on http://localhost:3000 && npm run test:production-ready",
    "monitor:setup": "echo 'Setting up monitoring tools...'",
    "bundle:analyze": "cross-env ANALYZE=true npm run build"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
